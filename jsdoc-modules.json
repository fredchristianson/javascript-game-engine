{
  "plugins": ["plugins/markdown"],
  "sourceType": "module",
  "source": {
    "include": ["browser/modules"],
    "includePattern": ".js$"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "opts": {
    "destination": "./docs/modules",
    "recurse": true
  }
}
